{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u256d\u2500\ueb99 {{ .UserName }} at",
          "foreground": "#ad60f4",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "mapped_locations": {
              "C:/Users/juls/*": "~"
            },
            "style": "full"
          },
          "template": " \uf07b {{ .Path }} ",
          "foreground": "#ad8ee6",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "in {{ .UpstreamIcon }}{{ .HEAD }} {{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 <#604d14>{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}</> <#f1c232>\uf046</><#604d14>{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}</> <#f1c232>\ueb4b</> <#604d14>{{ .StashCount }}{{ end }}</>",
          "foreground": "#f1c232",
          "powerline_symbol": "\ue0b0",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "always",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "template": " \ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#44903d",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .FormattedMs }} \uf252 ",
          "foreground": "#ad60f4",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}{{ .Meaning }}{{ else }}âœ”{{ end }} ",
          "foreground": "#d3d7cf",
          "type": "exit",
          "style": "plain",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .CurrentDate | date .Format }} \uf017\u2500\u256e",
          "foreground": "#ad60f4",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#ad60f4",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\ue285 ",
          "foreground": "#e0f8ff",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
